#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/astro@5.1.7_@types+node@22.10.7_jiti@1.21.7_lightningcss@1.28.2_rollup@2.79.2_stylus@0._8f6827f6b8273dd89f2201d110db67f8/node_modules/astro/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/astro@5.1.7_@types+node@22.10.7_jiti@1.21.7_lightningcss@1.28.2_rollup@2.79.2_stylus@0._8f6827f6b8273dd89f2201d110db67f8/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/astro@5.1.7_@types+node@22.10.7_jiti@1.21.7_lightningcss@1.28.2_rollup@2.79.2_stylus@0._8f6827f6b8273dd89f2201d110db67f8/node_modules/astro/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/astro@5.1.7_@types+node@22.10.7_jiti@1.21.7_lightningcss@1.28.2_rollup@2.79.2_stylus@0._8f6827f6b8273dd89f2201d110db67f8/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../astro/astro.js" "$@"
fi
