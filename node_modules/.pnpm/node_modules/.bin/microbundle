#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules/microbundle/dist/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules/microbundle/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules/microbundle/dist/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules/microbundle/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/microbundle@0.15.1/node_modules:/Users/rafif/Code/rafifdna.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../microbundle/dist/cli.js" "$@"
else
  exec node  "$basedir/../microbundle/dist/cli.js" "$@"
fi
